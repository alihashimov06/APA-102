@model Category



<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Dtail Table</h4>
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>
                                    Id
                                </th>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Product Count
                                </th>
                                <th>
                                    Creating
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                                <tr>
                                    <td>
                                        <p>@Model.Id</p>
                                    </td>
                                    <td>
                                        <p>@Model.Name</p>
                                    <td>
                                    <p>@Model.Products.Count</p>
                                    </td>
                                    <td>
                                    <p>@Model.CreatedAt</p>
                                    </td>
                                </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>